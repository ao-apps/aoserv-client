  <!-- Schemas have public fields that match the capitalized database table name -->
  <suppress
    checks="MemberName"
    files="[/\\]Schema\.java$"
    message="'([A-Z][a-z]+)+'"
  />

  <!-- Allowing underscore separators for methods and data that span database joins -->
  <suppress
    checks="(LocalVariableName|MemberName|ParameterName)"
    message="'[a-z]([a-z0-9][a-zA-Z0-9]*)?(_[a-z]([a-z0-9][a-zA-Z0-9]*)?)*'"
  />

